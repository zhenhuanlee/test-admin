{
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": [2, "never"],
    "space-before-function-paren": ["error", "always"],
    "object-curly-newline": ["error", { "multiline": false, "consistent": true }],
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never"
      }
   ]
  }
}
